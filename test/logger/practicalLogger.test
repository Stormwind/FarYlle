package require tcltest
namespace import ::tcltest::*

source logger/PracticalLogger.tcl

test createPracticalLogger {} -body {
  set logger [PracticalLogger new]
  info object class $logger
} -result "::PracticalLogger"

test getDebug {} -body {
  $logger getDebug
} -result 0

test setDebugOn {} -body {
    $logger setDebug 1
} -result 1

cleanupTests